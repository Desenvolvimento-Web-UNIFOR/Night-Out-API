generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_CLIENTE")
}

model Cliente {
  id_usuario      String   @id @default(uuid())
  apelido         String
  preferencias    String
  data_nascimento String
  created_at      DateTime @default(now())
}
